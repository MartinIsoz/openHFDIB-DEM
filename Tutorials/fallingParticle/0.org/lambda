FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      lambda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    z1
    {
        //~ type            symmetry;
        type            fixedValue;
        value           uniform 0;
    }
    z0
    {
        //~ type            symmetry;
        type            fixedValue;
        value           uniform 0;
    }
    x1
    {
        //~ type            zeroGradient;
        type            fixedValue;
        value           uniform 0;
    }
    x0
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        //~ type            zeroGradient;
        type            zeroGradient;
    }
}


